    {
        "name": "CAL__CREATE_SCHEDULE",
        "description": "Create a new schedule in Cal.com",
        "tags": [
            "schedule"
        ],
        "visibility": "public",
        "active": true,
        "protocol": "rest",
        "protocol_data": {
            "method": "POST",
            "path": "/v2/schedules",
            "server_url": "https://api.cal.com",
            "content_type": "application/json",
            "headers": {
                "cal-api-version": "2024-06-11"
            }
        },
        "parameters": {
            "type": "object",
            "properties": {
                "body": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string",
                            "description": "Name of the schedule"
                        },
                        "timeZone": {
                            "type": "string",
                            "description": "Timezone for the schedule in IANA format",
                            "example": "Asia/Shanghai"
                        },
                        "availability": {
                            "type": "array",
                            "description": "Availability time slots for the schedule",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "days": {
                                        "type": "array",
                                        "description": "Days of the week for this availability",
                                        "items": {
                                            "type": "string",
                                            "enum": [
                                                "Monday",
                                                "Tuesday",
                                                "Wednesday",
                                                "Thursday",
                                                "Friday",
                                                "Saturday",
                                                "Sunday"
                                            ]
                                        }
                                    },
                                    "startTime": {
                                        "type": "string",
                                        "description": "Start time in HH:MM format",
                                        "example": "09:00"
                                    },
                                    "endTime": {
                                        "type": "string",
                                        "description": "End time in HH:MM format",
                                        "example": "17:00"
                                    }
                                }
                            }
                        },
                        "isDefault": {
                            "type": "boolean",
                            "description": "Whether this is the default schedule",
                            "default": false
                        }
                    },
                    "required": [
                        "name",
                        "timeZone"
                    ],
                    "visible": [
                        "name",
                        "timeZone",
                        "availability",
                        "isDefault"
                    ],
                    "additionalProperties": false
                }
            },
            "required": [
                "body"
            ],
            "visible": [
                "body"
            ],
            "additionalProperties": false
        }
    },
