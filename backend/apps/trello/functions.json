    "name": "TRELLO__CREATE_CARD",
    "description": "Create a new card in Trello. You can specify the card name, description, position, labels, members and other attributes.",
    "tags": [
      "cards",
      "create"
    ],
    "visibility": "public",
    "active": true,
    "protocol": "rest",
    "protocol_data": {
      "method": "POST",
      "path": "/cards",
      "server_url": "https://api.trello.com/1"
    },
    "parameters": {
      "type": "object",
      "properties": {
        "query": {
          "type": "object",
          "description": "Query parameters",
          "properties": {
            "token": {
              "type": "string",
              "description": "Trello API token for authentication"
            },
            "name": {
              "type": "string",
              "description": "The name of the card"
            },
            "desc": {
              "type": "string",
              "description": "The description of the card"
            },
            "pos": {
              "type": "string",
              "description": "The position of the card. Valid values: 'top', 'bottom', or a positive number."
            },
            "due": {
              "type": "string",
              "description": "Date due in ISO 8601 format, or null"
            },
            "start": {
              "type": "string",
              "description": "Start date in ISO 8601 format, or null"
            },
            "idList": {
              "type": "string",
              "description": "The ID of the list to add the card to"
            },
            "idMembers": {
              "type": "array",
              "description": "Array of member IDs to add to the card",
              "items": {
                "type": "string"
              }
            },
            "idLabels": {
              "type": "array",
              "description": "Array of label IDs to add to the card",
              "items": {
                "type": "string"
              }
            }
          },
          "required": [
            "token",
            "idList"
          ],
          "visible": [
            "token",
            "name",
            "desc",
            "pos",
            "due",
            "start",
            "idList",
            "idMembers",
            "idLabels"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "query"
      ],
      "visible": [
        "query"
      ],
      "additionalProperties": false
    }
