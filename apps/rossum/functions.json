    "method": "GET",
    "path": "/api/v1/documents/{document_id}",
    "server_url": "https://{domain}.rossum.app"
  },
  "parameters": {
    "type": "object",
    "properties": {
      "path": {
        "type": "object",
        "properties": {
          "document_id": {
            "type": "string",
