          },
          "domain": {
            "type": "string",
            "description": "Tenant domain name"
          }
        },
        "required": ["document_id", "domain"],
        "visible": ["document_id", "domain"],
        "additionalProperties": false
      }
    },
    "required": ["path"],
    "visible": ["path"],
    "additionalProperties": false
  }
}]