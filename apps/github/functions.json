                            "default": 30
                        },
                        "page": {
                            "type": "integer",
                            "description": "The page number of the results to fetch",
                            "default": 1
                        }
                    },
                    "required": [],
                    "visible": ["per_page", "page"],
                    "additionalProperties": false
                }
            },
            "required": ["header", "path"],
            "visible": ["path", "query"],
            "additionalProperties": false
        }
